{
	"name": "filebox",
	"publisher": "qinghuangniao",
	"displayName": "filebox",
	"description": "save file with password.",
	"version": "0.1.2",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "filebox.startTimer",
				"title": "Start Timer"
			},
			{
				"command": "filebox.pauseTimer",
				"title": "Pause Timer"
			},
			{
				"command": "filebox.resetTimer",
				"title": "Reset Timer"
			}
		],
		"configuration": {
			"type": "object",
			"title": "filebox Configuration",
			"properties": {
				"filebox.workTime": {
					"type": "number",
					"default": 25,
					"description": "Time of work, in minutes."
				},
				"filebox.pauseTime": {
					"type": "number",
					"default": 5,
					"description": "Time of pause, in minutes"
				},
				"filebox.repeat": {
					"type": "number",
					"default": 4,
					"description": "Number of repeat before long break"
				}
			}
		},
		"snippets": [
			{
				"language": "markdown",
				"path": "./snippets/markdown.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/jsx.json"
			}
		]
	},
	"homepage": "https://github.com/janostudio/vscode-filebox/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/janostudio/vscode-filebox/issues",
		"email": "qinghuangniao@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janostudio/vscode-filebox.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
